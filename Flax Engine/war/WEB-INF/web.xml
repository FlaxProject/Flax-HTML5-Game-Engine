<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<servlet>
		<servlet-name>fileHandleServiceImpl</servlet-name>
		<servlet-class>ie.flax.flaxengine.server.FileHandleServiceImpl</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>remoteLoggerServiceImpl</servlet-name>
		<servlet-class>com.allen_sauer.gwt.log.server.RemoteLoggerServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileHandleServiceImpl</servlet-name>
		<url-pattern>/flax_engine/FileHandleService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>remoteLoggerServiceImpl</servlet-name>
		<url-pattern>/flax_engine/gwt-log</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Flax_Engine.html</welcome-file>
	</welcome-file-list>
</web-app>